int set_fragment_shader
(
    const fragment_shader_desc_t *desc,
    const uint32_t data_storage,
    uint32_t *fifo_buffer,
    uint32_t hw_sub 
)
{
	uint32_t *ptr = fifo;
	uint32_t Nv3D = hw_subch;

	return ptr - fifo;
}
